@model EntitiLayer.Concreate.Admin
@{
    ViewBag.Title = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h1>Yeni Admin Ekleme Sayfası</h1>
<br />
@using (Html.BeginForm("AddAdmin", "Authorization", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Admin Kukllanıcı Adı")
        @Html.TextBoxFor(x => x.AdminUsername, new { @class = "form-control" })
        <br /> <br />
        @Html.Label("şifreyi Girirniz")
        @Html.TextBoxFor(x => x.AdminPassword, new { @class = "form-control" })
        <br />  <br />
        @Html.Label("Admin Rol Girirniz")
        @Html.TextBoxFor(x => x.adminRole, new { @class = "form-control" })
        <br />  <br />
        <button class="btn btn-info">Kaydet</button>
    </div>
}
